#include<iostream>
#include<cmath>
using namespace std;
int main()
{
    int n,i,x;    
    cin>>n;
    for(i=1;i<=n;i++)
    {
        x=0;
        if(i%2==0)
            {            
            x=pow(i,2)-2;
            cout<<x<<" ";
        }
        else
            {            
            x=pow(i,2)-1;
            cout<<x<<" ";
        }
    }
}
